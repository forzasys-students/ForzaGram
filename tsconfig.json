{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "node16",
    "module": "Node16",
    "strict": true,
    "jsx": "react-native",           
    "allowJs": true,
    "noEmit": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
